### JavaScript:

```javascript
// Verlauf verwalten (max. 20 EintrÃ¤ge)
const MAX_HISTORY = 20;

function addToHistory(item) {
  let history = JSON.parse(localStorage.getItem('viewHistory') || '[]');
  
  // Duplikate entfernen
  history = history.filter(h => h.id !== item.id);
  
  // Neuen Eintrag am Anfang
  history.unshift({
    id: item.id,
    title: item.title,
    category: item.category,
    type: item.type, // 'wissensbasis' oder 'meine_dokumente'
    timestamp: Date.now()
  });
  
  // Auf MAX begrenzen
  history = history.slice(0, MAX_HISTORY);
  
  localStorage.setItem('viewHistory', JSON.stringify(history));
}

function getHistory() {
  return JSON.parse(localStorage.getItem('viewHistory') || '[]');
}

function clearHistory() {
  if (confirm('Verlauf wirklich lÃ¶schen?')) {
    localStorage.removeItem('viewHistory');
    renderHistory();
  }
}

// Zeitanzeige formatieren
function formatTimeAgo(timestamp) {
  const seconds = Math.floor((Date.now() - timestamp) / 1000);
  
  if (seconds < 60) return 'gerade eben';
  if (seconds < 3600) return `vor ${Math.floor(seconds / 60)} Minuten`;
  if (seconds < 86400) return `vor ${Math.floor(seconds / 3600)} Stunden`;
  if (seconds < 172800) return 'gestern';
  return `vor ${Math.floor(seconds / 86400)} Tagen`;
}
```

